/**
 * THON DOCS
 *
 * ATTENTION:
 * This module file is generated automatically, do not change it.
 *
 * To update it according your new files runs on terminal:
 *   $ thon build
 */
<% props.components.forEach(function(component){ %>
import * as <%= component.componentName %> from './<%= component.relativePath %>';<% }); %>

export const modules = {<% props.components.forEach(function(component){ %>
  <%= component.componentName %>,<% }); %>
};

export const markdowns = {<% props.markdowns.forEach(function(markdown){ %>
  '<%= markdown.slug %>': require('./<%= markdown.path %>'),<% }); %>
};

export const structure = <%- JSON.stringify(props.structure, null, 2) %>;

const thonDocs = {
  modules,
  markdowns,
  structure,
};

export default thonDocs;